{
  "name": "react-web-sdk",
  "version": "0.0.1",
  "description": "UI SDK based on react-native",
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rm -rf ./dist && npm install",
    "build": "webpack --config webpack.config.js",
    "build:watch": "npm run build -- --watch",
    "build:example": "npm run build && cd example && webpack --config ./webpack.config.js",
    "build:example:watch": "npm run build:example -- --watch"
  },
  "dependencies": {
    "react": "^0.13.3"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.0",
    "babel-core": "^5.5.6",
    "babel-loader": "^5.1.4",
    "babel-runtime": "^5.5.6",
    "css-loader": "^0.15.1",
    "extract-text-webpack-plugin": "^0.8.1",
    "node-libs-browser": "^0.5.2",
    "postcss-loader": "^0.4.4",
    "postcss-modules-local-by-default":  "^v0.0.10",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.10"
  },
  "author": "Nicolas Gallagher",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/necolas/react-web-sdk.git"
  }
}
