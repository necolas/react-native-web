    "react-native-web": "0.9.8"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "babel-plugin-react-native-web": "0.9.8",
    "file-loader": "^1.1.11",
    "webpack": "^4.8.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.1.3"
  }
}
