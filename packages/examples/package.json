{
  "private": true,
  "name": "react-native-examples",
  "version": "0.10.0",
  "scripts": {
    "build": "make-dir dist && cp -f src/index.html dist/index.html && ./node_modules/.bin/webpack-cli --config ./webpack.config.js",
