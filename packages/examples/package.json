  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.2.0",
    "babel-plugin-react-native-web": "0.10.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "file-loader": "^1.1.11",
    "make-dir-cli": "^2.0.0",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  }
}
