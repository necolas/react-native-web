{
  "private": true,
  "name": "react-native-examples",
  "version": "0.9.8",
  "scripts": {
    "build": "mkdir -p dist && cp -f src/index.html dist/index.html && ./node_modules/.bin/webpack-cli --config ./webpack.config.js",
